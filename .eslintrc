{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 9,
        "ecmaFeatures": {
            "jsx": true,
        },
        "sourceType": "module",
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
    },
    "plugins": [
        "react",
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
    ]
}
